{
    "name": "bitnoise/node-bundle",
    "type": "symfony-bundle",
    "description": "",
    "keywords": [ "bitnoise", "cms", "tree"],
    "homepage": "https://github.com/Bitnoise/BtnNodeBundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Bitnoise Team",
            "homepage": "http://www.bitnoi.se/",
            "email": "labs@bitnoi.se"
        }
    ],
    "require": {
        "symfony-cmf/routing-bundle": "~1.2.0",
        "bitnoise/admin-bundle": "2.0.*"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "~2.0.0",
        "fabpot/php-cs-fixer": "~0.5.0",
        "phpmd/phpmd" : "~2.0.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://bitnoise.github.io/packages/"
        }
    ],
    "autoload": {
        "psr-4": { "Btn\\NodeBundle\\": "src/Btn/NodeBundle/" }
    },
    "extra": {
        "branch-alias": {
            "dev-2.0": "2.0-dev"
        }
